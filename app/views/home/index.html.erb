<div class="col-md-offset-2 col-md-8 cv-title text-center mTop-20">
  <h3>Select a Location to find an event</h3>
</div>
<div class="col-md-offset-2 col-md-8 form-group" style="display:inline;">
  <%= form_tag(search_path, method: "get",:id=>"search") do %>
    <div class="input-group" style="display:table;">
      <span class="input-group-addon" style="width:1%;"><span class="fa fa-search"></span></span>
      <input type="text" name="location" id="location" placeholder="Search for Location, Videos, Events etc.,"  class="form-control cv-search location">
      <input type="hidden" name="place_id" id="place_id"/>
      <div id="demo" style="color:#fff;"></div>
    </div>
  <%end%>
</div>
<%=include_gon%>
